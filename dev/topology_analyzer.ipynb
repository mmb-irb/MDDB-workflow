   "source": [
    "import prody\n",
    "\n",
    "topology_filename = 'md.imaged.rot.dry.pdb'\n",
    "\n",
    "topology = prody.parsePDB(topology_filename)\n",
    "chains = topology.iterChains()\n",
    "for chain in chains:\n",
    "    name = chain.getChid()\n",
    "    sequence = chain.getSequence(allres=True)\n",
    "    print(name + ' -> ' + ''.join(sequence))\n"
   ]
